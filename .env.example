# WYND MCP Server Configuration
# Copy this file to .env and update the values

# API Configuration
WYND_API_URL=https://wynd.mmogomedia.com
WYND_API_TOKEN=your_api_token_here

# Default project ID (optional)
# DEFAULT_PROJECT_ID=proj_1234567890

# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Timeout for API requests in milliseconds
API_TIMEOUT=10000

# Optional: Database configuration (if needed)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=wynd_mcp
# DB_USER=postgres
# DB_PASSWORD=postgres
# DB_SSL=false
